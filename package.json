{
    "name": "@caspahouzer/ti-jsondb",
    "version": "1.0.12",
    "description": "A file based JSON database for TiDev Titanium",
    "main": "ti-jsondb.js",
    "scripts": {
        "docs": "jsdoc2md --template methods.hbs ti-jsondb.js > methods.md",
        "test": "grunt test",
        "patch": "grunt test && npm version patch -m 'bump version' && npm publish && git push && git push --tags",
        "minor": "grunt test && npm version minor -m 'bump version' && npm publish && git push && git push --tags",
        "major": "grunt test && npm version major -m 'bump version' && npm publish && git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/caspahouzer/ti-jsondb.git"
    },
    "publishConfig": {
        "access": "public",
        "name": "ti-jsondb",
        "registry": "https://npm.pkg.github.com/caspahouzer"
    },
    "keywords": [
        "appcelerator",
        "titanium",
        "titanium-mobile",
        "json",
        "database",
        "helper"
    ],
    "author": "Sebastian Klaus",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/caspahouzer/ti-jsondb/issues"
    },
    "homepage": "https://github.com/caspahouzer/ti-jsondb#readme",
    "devDependencies": {
        "fs-extended": "^0.2.1",
        "grunt": "^1.4.1",
        "grunt-mocha-test": "^0.13.3",
        "jsdoc-to-markdown": "^7.1.1",
        "mocha": "^9.2.2",
        "should": "^13.2.3",
        "underscore": "^1.13.2"
    }
}