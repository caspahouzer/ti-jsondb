{
    "name": "@caspahouzer/ti-jsondb",
    "version": "1.0.5",
    "description": "A file based JSON database for TiDev Titanium",
    "main": "index.js",
    "scripts": {
        "docs": "jsdoc2md --template methods.hbs index.js > methods.md",
        "commonjs": "mkdir -p commonjs && cp index.js commonjs/ti-jsondb.js",
        "prepublish": "npm run docs && npm run commonjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/caspahouzer/ti-jsondb.git"
    },
    "publishConfig": {
        "access": "public",
        "name": "ti-jsondb",
        "registry": "https://npm.pkg.github.com/caspahouzer"
    },
    "keywords": [
        "appcelerator",
        "titanium",
        "titanium-mobile",
        "json",
        "database",
        "helper"
    ],
    "author": "Sebastian Klaus",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/caspahouzer/ti-jsondb/issues"
    },
    "homepage": "https://github.com/caspahouzer/ti-jsondb#readme",
    "dependencies": {
        "compress-json": "^2.0.1",
        "zipson": "^0.2.12"
    },
    "devDependencies": {
        "jsdoc-to-markdown": "^7.1.1"
    }
}
